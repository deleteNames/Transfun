<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="aTitle" format="string" />
        <attr name="aIcon" format="reference" />
    </declare-styleable>
    <declare-styleable name="Loading">
        <attr format="dimension" name="gBackgroundLineSize"/>
        <attr format="dimension" name="gForegroundLineSize"/>
        <attr format="reference|color" name="gBackgroundColor"/>
        <attr format="reference|color" name="gForegroundColor"/>
        <attr format="boolean" name="gAutoRun"/>
        <attr format="float" name="gProgressFloat"/>
    </declare-styleable>


    <declare-styleable name="CirclePagerIndicator">
        <attr name="circle_indicator_centerHorizontal" format="boolean" />
        <attr name="circle_indicator_follow" format="boolean" />
        <attr name="circle_indicator_orientation" format="reference" />
        <attr name="circle_indicator_color" format="color" />
        <attr name="circle_indicator_stroke_color" format="color" />
        <attr name="circle_indicator_stroke_width" format="dimension" />
        <attr name="circle_indicator_fill_color" format="color" />
        <attr name="circle_indicator_radius" format="dimension" />
        <attr name="circle_indicator_space" format="dimension" />
        <attr name="circle_indicator_indicator_radius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="LinePagerIndicator">
        <attr name="line_indicator_centerHorizontal" format="boolean" />
        <attr name="line_indicator_width" format="dimension" />
        <attr name="line_indicator_height" format="dimension" />
        <attr name="line_indicator_indicator_height" format="dimension" />
        <attr name="line_indicator_space" format="dimension" />
        <attr name="line_indicator_fill_color" format="color" />
        <attr name="line_indicator_indicator_color" format="color" />
        <attr name="line_indicator_follow" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CircleBannerIndicator">
        <attr name="circle_banner_indicator_centerHorizontal" format="boolean" />
        <attr name="circle_banner_indicator_orientation" format="reference" />
        <attr name="circle_banner_indicator_color" format="color" />
        <attr name="circle_banner_indicator_stroke_color" format="color" />
        <attr name="circle_banner_indicator_stroke_width" format="dimension" />
        <attr name="circle_banner_indicator_fill_color" format="color" />
        <attr name="circle_banner_indicator_radius" format="dimension" />
        <attr name="circle_banner_indicator_space" format="dimension" />
        <attr name="circle_banner_indicator_indicator_radius" format="dimension" />
    </declare-styleable>

    <attr name="verticalSpace" format="dimension" />
    <attr name="horizontalSpace" format="dimension" />

    <declare-styleable name="DetailPicturesLayout">
        <attr name="verticalSpace" />
        <attr name="horizontalSpace" />
        <attr name="column" format="integer" />
        <attr name="maxPictureSize" format="dimension" />
    </declare-styleable>

    <declare-styleable name="ToggleButton">
        <attr name="toggle_border_width" format="dimension">
        </attr>
        <attr name="offBorderColor" format="reference|color">
        </attr>
        <attr name="offColor" format="reference|color">
        </attr>
        <attr name="onColor" format="reference|color">
        </attr>
        <attr name="spotColor" format="reference|color">
        </attr>
    </declare-styleable>

    <declare-styleable name="SquareLayout">
        <attr format="integer" name="oscAccordTo">
            <enum name="width" value="1"/>
            <enum name="height" value="2"/>
            <enum name="useMin" value="3"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BannerView">
        <attr name="horizontal_offset" format="dimension" />
        <attr name="vertical_offset" format="dimension" />
        <attr name="scroll_duration" format="integer" />
    </declare-styleable>

    <!-- MatchSupportProgressBar -->
    <declare-styleable name="MatchSupportProgressBar">
        <!-- 进度条圆角角度 -->
        <attr name="progress_radio" format="string"></attr>
        <!-- 进度条左侧Tag文本 -->
        <attr name="progress_tag_text" format="string"></attr>
        <!-- 进度条已填充部分颜色 -->
        <attr name="progress_reach_color" format="color"></attr>
        <!-- 进度条已填充部分高度 -->
        <attr name="progress_reach_height" format="dimension"></attr>
        <!-- 进度条背景颜色 -->
        <attr name="progress_background_color" format="color"></attr>
        <!-- 进度条背景高度 -->
        <attr name="progress_background_height" format="dimension"></attr>
        <!-- 进度条左侧Tag文本颜色 -->
        <attr name="progress_tag_text_color" format="color"></attr>
        <!-- 进度条左侧Tag文本尺寸 -->
        <attr name="progress_tag_text_size" format="dimension"></attr>
        <!-- 进度条右侧百分比文本颜色 -->
        <attr name="progress_percent_text_color" format="color"></attr>
        <!-- 进度条右侧百分比文本尺寸 -->
        <attr name="progress_percent_text_size" format="dimension"></attr>
        <!-- 进度条右侧百分比文本 -->
        <attr name="progress_percent_text" format="string"></attr>
        <!-- 进度条选中的图标与进度条、文字的间距 -->
        <attr name="progress_offsetX" format="dimension"></attr>
        <!-- 进度条右侧文字的水平偏移量 -->
        <attr name="progress_offsetR" format="dimension"></attr>
        <!-- 进度条之间的偏移量 -->
        <attr name="progress_LineSpacing" format="dimension"></attr>
    </declare-styleable>
</resources>